@model Everestfcsweb.Entities.DiscountListModelData
<div class="modal-header">
    <h5 class="text-sm modal-title" id="SocialPesaModalLabel">Discount List</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body">
    <div class="row">
        <div class="col-sm-12">
            <input asp-for="@Model.DiscountListId" id="SystemDiscountListId" hidden />
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <div class="form-group">
                <label class="font-weight-light">Discount List Name</label>
                <input class="form-control form-control-sm" asp-for="@Model.DiscountListname" required id="SystemDiscountListnameId" />
            </div>
        </div>
    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-sm btn-danger" data-dismiss="modal">Close</button>
    <button class="btn btn-sm btn-info font-weight-bold" type="button" onclick="savesystemdiscountlistdetaildata();">Save</button>
</div>