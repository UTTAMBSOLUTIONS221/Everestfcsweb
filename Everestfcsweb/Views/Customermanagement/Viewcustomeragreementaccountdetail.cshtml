@model Everestfcsweb.Models.SystemAccountDetailData
@{
    ViewData["Title"] = "Viewcustomeragreementaccountdetail";
}

<div class="row">
    <div class="col-sm-12">
        <div class="card">
            <div class="card-header py-1">
                <div class="row">
                    <div class="col-sm-9">
                        <h6 class="font-weight-bold text-sm card-title pt-1 text-uppercase">Account Topups/Transfers</h6>
                    </div>
                    <div class="col-sm-3">
                        <a asp-action="AddcustomeragreementAccountTopup" asp-controller="CustomerManagement" asp-route-AccountId="@Model.AccountId" asp-area="" class="btn btn-xs btn-info float-right" data-target="#FuelcardsystemModalLarge" data-toggle="modal" data-backdrop="static" data-keyboard="false">Topup</a>
                    </div>
                </div>
            </div>
            <div class="card-body">

            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-sm-12">
        <div class="card">
            <div class="card-header py-1">
                <div class="row">
                    <div class="col-sm-9">
                        <h6 class="font-weight-bold text-sm card-title pt-1 text-uppercase">Account </h6>
                    </div>
                    <div class="col-sm-3">
                        <a asp-action="AddcustomeragreementAccountTopup" asp-controller="CustomerManagement" asp-route-AccountId="@Model.AccountId" asp-area="" class="btn btn-xs btn-info float-right" data-target="#FuelcardsystemModalLarge" data-toggle="modal" data-backdrop="static" data-keyboard="false">Topup</a>
                    </div>
                </div>
            </div>
            <div class="card-body">
            </div>
        </div>
    </div>
</div>